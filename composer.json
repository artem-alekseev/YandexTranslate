{
  "name": "artem-alekseev/yandex-translate",
  "type": "library",
  "description": "Yandex Translate",
  "license": "MIT",
  "authors": [
    {
      "name": "Artem Alekseev",
      "email": "lexx73652@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "spatie/laravel-translatable": "^6.10",
    "illuminate/database": "^9.0|^10.0|^11.0",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64"
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "keywords": [
    "yandex",
    "translate",
    "YandexTranslate"
  ],
  "autoload": {
    "psr-4": {
      "YandexTranslate\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YandexTranslate\\YandexTranslateServiceProvider"
      ]
    }
  }
}
